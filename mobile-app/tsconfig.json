{
    "compilerOptions": {
        "jsx": "react",
        "outDir": "./dist/",
        "noImplicitAny": false,
        "module": "commonjs",
        "sourceMap": true,
        "strictNullChecks": true,
        "baseUrl": ".",
        "paths": {
            "@": [
                "./*"
            ]
        }
    },
    "extends": [
        "expo/tsconfig.base",
        "@tsconfig/react-native/tsconfig.json"
    ],
    "exclude": [
        "dist",
        "build",
        "node_modules"
    ]
}